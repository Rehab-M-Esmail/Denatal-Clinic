@page
@model dental_clinic.Pages.Doctors

@{
    Layout = "Shared/_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <title>Information</title>
</head>
<body>
<table class="table table-striped table-bordered">
    <thead>
    <tr class="table-primary">
        <th>Doctor name</th>
        <th>Doctor Email</th>
    </tr>
    </thead>
    <tbody>
    @for (int i = 0; i < @Model.dt.Rows.Count; i++)
    {
        <tr>
            @* <th scope="row">@i</th> *@
            @for (int j = 0; j < @Model.dt.Columns.Count; j++)
            {
                <td class="table-primary">@Model.dt.Rows[i][j]</td>
            }
        </tr>
    }
    </tbody>
</table>
</body>
</html>