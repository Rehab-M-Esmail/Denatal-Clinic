@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>
<h1>Contact Us</h1>
<table>
   <thead>
    <tr>
        <th> Reception name :</th>
        <th>Contact number: </th>
    </tr>
    </thead>
<tbody>
@for (int i = 0; i < @Model.dt.Rows.Count; i++)
{
    <tr>
    @for (int j = 0; j < @Model.dt.Columns.Count; j++)
    {
        <td>@Model.dt.Rows[i][j]</td>
    }
    </tr>
}
</tbody>
</table>

